    {"name": "useUserCustomColor", "type": "toggle", "default": true},
    {"name": "clearBannedMsg", "type": "toggle", "default": true},
    {"name": "allowClearChat", "type": "toggle", "default": true},
    {"name": "textareaDummy", "type": "textarea", "default": "Textarea\nsecond line"},
    {"name": "textDummy", "type": "text", "default": "Text"},
    {"name": "optionDummy", "type": "select",
      "values":["option1","option2","option3"], "default": "option2" 
    },
    {"name": "multiOptionDummy", "type": "multiselect",
      "values":["option1","option2","option3","option4","option5"], "default": ["option2" ,"option5"]
    }

  ]
